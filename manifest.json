{
  "name":"webitv2",
  "version":"0.0.2",
  "manifest_version":2,
  "description":"marutaru",
  "browser_action":{
    "default_popup":"main.html"
  },
  "permissions":[
    "tabs"
  ],
  "content_scripts":[
    {
      "matches":["http://*/*"],
      "js":["js/jquery.js","js/bootstrap.min.js","js/socket.io.js","js/note.js"],
      "css":["css/bootstrap.min.css","css/note.css"],
      "run_at":"document_start"
    }
  ]
}
